# Supabase Postgres connection (Session pooler recommended)
DB_HOST=aws-1-ap-south-1.pooler.supabase.com
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres.<your-project-ref>
DB_PASSWORD=YOUR_SUPABASE_DB_PASSWORD

# Set to False if your network intercepts TLS with a custom CA
DB_SSL_VERIFY=False

# Create tables automatically on startup (dev only)
DB_AUTO_CREATE=False

DEBUG=True
APP_NAME=Web-Spy
APP_VERSION=1.0.0
CORS_ORIGINS=["http://localhost:5173"]
